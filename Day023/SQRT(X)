--CODE
class Solution {
public:
    int mySqrt(int x) {
       int start=0,end=x,ans=0;
       while(start<=end){
        int mid=start+(end-start)/2;
        long long square=1LL*mid*mid;
          if(square==x) {
          ans=mid;
          return mid;
          }
          else if (square>x){
            ans=mid-1;
            end=mid-1;
           }
           else start=mid+1;
       }
       return ans;
    }
};

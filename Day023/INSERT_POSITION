leetcode-35
stored position if the mid eleemnt is greater than target
 -- code

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int start=0,end=nums.size()-1,pos=nums.size();
        while(start<=end){
            int mid=start+(end-start)/2;
            if(nums[mid]==target){
                pos=mid;
                return pos;
            }
            else if(nums[mid]>target){
                pos=mid;
                end=mid-1;
            }else{
                start=mid+1;
            }
        }
        return pos;
    }
};

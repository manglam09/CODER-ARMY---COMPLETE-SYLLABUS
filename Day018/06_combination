#include <iostream>
using namespace std;

long long factorial(int n) {
    long long fact = 1;
    for (int i = 1; i <= n; i++) {
        fact *= i;
    }
    return fact;
}

long long nCr(int n, int r) {
    return factorial(n) / (factorial(r) * factorial(n - r));
}

int main() {
    int n, r;
    cout << "Enter n and r: ";
    cin >> n >> r;

    if(r > n) {
        cout << "Invalid input! r should be <= n." << endl;
    } else {
        cout << "nCr = " << nCr(n, r) << endl;
    }

    return 0;
}

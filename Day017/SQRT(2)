class Solution {
public:
    int mySqrt(int x) {
        int start=0 , end=x , ans=0;
        if(x<=1) return x;
        while(start<=end){
            int mid=start+(end-start)/2;
            long long square=1LL*mid*mid;
            if(square==x) {
                ans=mid;
                return ans;
            }
            else if(square<x){
                ans=mid;
                 start=mid+1;
            }
            else end=mid-1;
        }
        return ans;
    }
};

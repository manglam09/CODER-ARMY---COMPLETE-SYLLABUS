
class Solution {
  public:
    long long squaresInChessBoard(long long N) {
        // code here
        long long ans=0;
        for(long long i=1;i<=N;i++){
            ans+=i*i;
        }
        return ans;
        
    }
};

--ANOTHER APPROACH USING F0RMULA--
class Solution {
  public:
    long long squaresInChessBoard(long long N) {
        return (N * (N + 1) * (2 * N + 1)) / 6;
    }
};

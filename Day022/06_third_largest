#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter size of array (n > 3): ";
    cin >> n;

    if (n <= 3) {
        cout << "Array size must be greater than 3" << endl;
        return 0;
    }

    int arr[n];
    cout << "Enter " << n << " unique elements: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    // Sort array (simple selection sort)
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] > arr[j]) {
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    cout << "Third smallest element is: " << arr[2] << endl;

    return 0;
}
